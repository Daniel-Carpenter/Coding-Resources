# -*- coding: utf-8 -*-
"""
Driver code for very basic object oriented program
"""

from MyMath import *
from MyAnalyticalMath import *
from Casting import *    
    
# Create basic object from MyMath class
out = MyMath(5, 4)

print(out)
    

# Proof of Inheritence
out2 = MyAnalyticalMath(5, 4)

print(out2)


myVar = 4
myVar2 = 1

myVar2
    
# Try catch
myCast = Casting('dog')

print(myCast)

