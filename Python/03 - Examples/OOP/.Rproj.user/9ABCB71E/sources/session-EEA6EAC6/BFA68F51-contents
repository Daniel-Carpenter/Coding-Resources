# -*- coding: utf-8 -*-
"""
Goal: try/cast implementation
"""

class Casting:
  
  def __init__(self, num1):
    self.num1 = num1
    
    
  def castAsInt(self):
    try:
      self.num1 = int(self.num1)
      print('Successful Cast')
      
    except:
      print('Cannot cast as int\nSupplied Value:', self.num1)
      
    return self.num1
      
      
  def __str__(self):
    return self.castAsInt()
      
  
